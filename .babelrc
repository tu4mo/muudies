{
  "presets": [
    ["latest", { "es2015": { "modules": false } } ],
    "react"
  ],
  "plugins": [
    ["transform-class-properties"],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}
